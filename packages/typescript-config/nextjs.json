{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "module": "esnext",
    "moduleResolution": "Bundler",
    "target": "esnext",
    "noImplicitAny": true,
    "allowJs": true,
    "jsx": "preserve",
    "noEmit": true
  }
}